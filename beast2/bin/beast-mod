#!/bin/sh

PAKG_LIB="$HOME/LPhyBeastTest/"
ls $PAKG_LIB

BEAST_LIB="$PAKG_LIB/beast2/lib"

java -Dbeast.debug=true -Dbeast.log.level=trace -Xmx8g -Dbeast.user.package.dir="$PAKG_LIB" -Djava.library.path="$BEAGLE_LIB_PATH" -Dlauncher.wait.for.exit=true -cp "$BEAST_LIB/launcher.jar" beast.pkgmgmt.launcher.BeastLauncher $*

