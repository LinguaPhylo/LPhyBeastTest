L = 50;
μ = 0.01;
lM = 3.0;
lS = 1.0;
alpha = 0.01;
beta = 0.01;
lambda ~ LogNormal(meanlog=lM, sdlog=lS);
Q = binaryRateMatrix(lambda=lambda);
ψ = newick(tree="((A:1,B:1):1,(C:0.5, D:0.5):1.5):0.0;");
ncat = 4;
shape = 0.75;
siteRates ~ DiscretizeGamma(shape=shape, ncat=ncat, replicates=L);
S ~ PhyloCTMC(siteRates=siteRates, mu=μ, Q=Q, tree=ψ);
D ~ ErrorModel(alpha=alpha, beta=beta, alignment=S);
nchar = D.nchar();
